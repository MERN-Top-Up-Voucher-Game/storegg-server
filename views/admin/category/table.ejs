<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% category.forEach((data, key) => { %>
    <tr>
      <td><%= key + 1%></td>
      <td><%= data.name %></td>
      <td>
        <form action="/category/edit" method="POST" class="form d-inline">
          <input type="hidden" name="id" value="<%= data._id%>" />
          <button class="btn btn-sm btn-warning">
            <i class="fas fa-pencil-alt"></i> Ubah
          </button>
        </form>

        <form
          action="/category/delete?_method=DELETE"
          method="POST"
          class="form d-inline"
        >
          <input type="hidden" name="id" value="<%= data._id%>" />
          <button class="btn btn-sm btn-danger">
            <i class="fas fa-trash"></i> Hapus
          </button>
        </form>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
